<h3>Employee List</h3>
<a class="btn btn-secondary" href="/employee/transaction">See Transaction</a>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Amount</th>
      <th>City</th>
    </tr>
  </thead>
  <tbody>
    {{#each list}}
    <tr>
      <td>{{this.fullname}}</td>
      <td>{{this.email}}</td>
      <td>{{this.amount}}</td>
      <td>{{this.address}}</td>
      <td>
        <a href="/employee/transfer/{{this._id}}">Transfer Money</a>
        <a href="/employee/userInfo/{{this._id}}">Info</a>
      </td>
    </tr>
    {{/each}}
  </tbody>
</table>